{{#if user}}
  <div>
    <section class="content-header">
      <h1>
        Default Configuration
      </h1>
      <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">User Default Configuration</li>
      </ol>
    </section>
  <section class="content">
  <div class="row">
    <div class="col-xs-12">
      <div class="box">
        <div class="box-body">
          <table id="example2" class="table table-bordered table-hover">
            <thead>
            <tr>
              <th>#</th>
              <th>Field</th>
              <th>Value</th>
            </tr>
            </thead>
            <tbody>
            <tr>
              <td></td>
              <td>E-Mail</td>
              <td>{{toJSONusername info}}</td>
            </tr>
            <tr>
              <td>1</td>
              <td>AWS Default Cluster Prefix</td>
              <td>{{toJSONname info}}</td>
            </tr>
            <tr>
              <td>2</td>
              <td>AWS Access Key ID</td>
              <td>{{toJSONawstoken info}}</td>
            </tr>
            <tr>
              <td>3</td>
              <td>AWS Secret Access Key</td>
              <td>{{toJSONawssecret info}}</td>
            </tr>
            <tr>
              <td>4</td>
              <td>AWS Key Pair</td>
              <td>{{toJSONawskeyname info}}</td>
            </tr>
            <tr>
              <td>5</td>
              <td>AWS Region</td>
              <td>{{toJSONawsregion info}}</td>
            </tr>
            <tr>
              <td>6</td>
              <td>AWS Subnet ID</td>
              <td>{{toJSONawssubnetid info}}</td>
            </tr>
            <tr>
              <td>7</td>
              <td>AWS Subnet ID 2</td>
              <td>{{toJSONawssubnetid2 info}}</td>
            </tr>
            <tr>
              <td>8</td>
              <td>AWS Security Group ID</td>
              <td>{{toJSONawssecurityid info}}</td>
            </tr>
            </tbody>
            <tfoot>
            <tr>
              <th>#</th>
              <th>Field</th>
              <th>Value</th>
            </tr>
            </tfoot>
          </table>
        </div>
        <!-- /.box-body -->
        <div>
          <br>
          <br>
          <a href="/edituserInfo">
            <table>
            <tr>
            <button type="button" class="btn btn-block btn-primary">Edit Default Configuration</button>
            </tr>
            </table>
          </a>
          <br>
          <a href="/deleteAccount">
            <table>
              <tr>
                <button type="button" class="btn btn-block btn-danger">Delete Account</button>
              </tr>
            </table>
          </a>
        </div>
      </div>
    </div>
  </div>
  </section>
    <!-- /.col-lg-12 -->

    <!-- Main content -->
    <!-- /.content -->
  </div>
  <script src="plugins/jQuery/jquery-2.2.3.min.js"></script>
  <!-- Bootstrap 3.3.6 -->
  <script src="bootstrap/js/bootstrap.min.js"></script>
  <!-- AdminLTE App -->
  <script src="dist/js/app.min.js"></script>
  <script src="plugins/sparkline/jquery.sparkline.min.js"></script>
  <!-- jvectormap -->
{{else}}
  <div style="height: 600px">
    <h1>Welcome to ScaleX</h1>
    <p>Please sign in to continue!</p>
    <p>
      <a href="/signin" class="btn btn-primary btn-lg" role="button">
        <span class="glyphicon glyphicon-user"></span>
        Sign in!
      </a>
    </p>
  </div>
{{/if}}
