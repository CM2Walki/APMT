  <script>
    $.post(
      "/awskubernetes/getLoadKubernetesData",{
        testName: "linearLoadTest"
      },
      function(data) {
        console.log(data);
        //data=JSON.parse(data);
        var testData = [];
        for (i=0;i<data.timestamp.length;i++)
        {
          var row = [data.timestamp[i],data.desiredReplicas[i],data.currentReplicas[i],data.avblReplicas[i]  ];
          testData.push(row);
        }
        new Dygraph(document.getElementById("autoscaleInstancesKube"),
          testData,
          {
            rollPeriod: 14,
            labels: ["timestamp", "Desired Replicas", "Current Replicas", "Available Replicas"],
            colors: ['#284785', '#EE1111', '#8AE234']
          });
        $("#overlayautoscaleInstancesKube").hide("slow");
        var testData = [];
        for (i=0;i<data.timestamp.length;i++)
        {
          var row = [data.timestamp[i],data.cpuPercentageCurrent[i],data.cpuPercentageAvg[i] ];
          testData.push(row);
        }
        new Dygraph(document.getElementById("linecpuUtilizationKube"),
          testData,
          {
            rollPeriod: 14,
            labels: ["timestamp", "CPU-Utilization Current", "CPU-Utilization Avg"],
            colors: ['#EE1111', '#8AE234']
          });
        $("#overlaylinecpuUtilizationKube").hide("slow");
      });

    $.post(
      "/awskubernetes/getautoScaleData",{
        testName: "linearLoadTest"
      },
      function(data) {
        console.log("Autoscale"+data);
        var testData = [];
        for (i=0;i<data.timestamp.length;i++)
        {
          var row = [data.timestamp[i],data.desiredInstances[i],data.currentInstances[i] ];
          testData.push(row);
        }
        new Dygraph(document.getElementById("autoscaleInstances"),
          testData,
          {
            rollPeriod: 14,
            labels: ["timestamp", "Desired Instances", "Current Instances"]
          });
        $("#overlayautoscaleInstances").hide("slow");
        var testData = [];
        for (i=0;i<data.timestamp.length;i++)
        {
          var row = [data.timestamp[i],data.cpuPercentage[i]];
          testData.push(row);
        }
        new Dygraph(document.getElementById("linecpuUtilization"),
          testData,
          {
            rollPeriod: 14,
            labels: ["timestamp", "CPU-Utilization"]
          });
        $("#overlaylinecpuUtilization").hide("slow");
      });

    $.post(
      "/awskubernetes/getLoadTestData",{
        testName: "linearLoadTest"
      },
      function(data) {
        var allData = data;
        console.log(allData);
        var testData = [];
        for (i=0;i<allData.datarequestElapsed.length;i++)
        {
            var row = [allData.requestIndex[i],allData.datarequestElapsed[i] ];
          testData.push(row);
        }
        new Dygraph(document.getElementById("line"),
          testData,
          {
            rollPeriod: 14,
            labels: ["index", "value"]
          });
        $("#overlayline").hide("slow");
        var testData = [];
        for (i=0;i<allData.datarequestElapsed.length;i++)
        {
          var row = [allData.requestIndex[i],allData.maxLatency[i],allData.meanLatency[i],allData.minLatency[i]  ];
          testData.push(row);
        }
        new Dygraph(document.getElementById("lineLatency"),
          testData,
          {
            rollPeriod: 14,
            labels: ["index", "Max Latency", "Mean Latency", "Min Latency"],
            colors: ['#284785', '#EE1111', '#8AE234']
          });
        $("#overlaylineLatency").hide("slow");
        var testData = [];
        for (i=0;i<allData.datarequestElapsed.length;i++)
        {
          var row = [allData.requestIndex[i],allData.requestIndex[i] ];
          testData.push(row);
        }
        new Dygraph(document.getElementById("barRPS"),
          testData,
          {
            rollPeriod: 14,
            labels: ["index", "Requests Per Second"]
          });
        $("#overlaybarRPS").hide("slow");
        var testData = [];
        for (i=0;i<allData.datarequestElapsed.length;i++)
        {
          var row = [allData.requestIndex[i],allData.totalTimeSeconds[i] ];
          testData.push(row);
        }
        new Dygraph(document.getElementById("lineLatencyTotal"),
          testData,
          {
            rollPeriod: 14,
            labels: ["index", "Total Latency Time (in Sec)"]
          });
        $("#overlaylineLatencyTotal").hide("slow");
        var testData = [];
        for (i=0;i<allData.datarequestElapsed.length;i++)
        {
          var row = [allData.requestIndex[i],allData.errors[i] ];
          testData.push(row);
        }
        new Dygraph(document.getElementById("barErrors"),
          testData,
          {
            rollPeriod: 14,
            labels: ["index", "Total Errors"]
          });
        $("#overlaybarErrors").hide("slow");
      }
    );
    $.post(
      "/awskubernetes/getLoadTestTimelineData",{
        testName: "linearLoadTest"
      },
      function(data) {
        console.log(data);
        var result = $.map(data, function(val, key) {

          if(val.kind == "Pod")
          {
            var obj ={
              name: key
            };

            if(val.Scheduled)
            {
              obj.scheduled = val.Scheduled.firstTimestamp;
            }
            if(val.FailedScheduling)
            {
              obj.FailedScheduling = val.FailedScheduling.firstTimestamp;
            }
            if(val.SuccessfulMountVolume)
            {
              obj.SuccessfulMountVolume = val.SuccessfulMountVolume.firstTimestamp;
            }
            if(val.Pulling)
            {
              obj.Pulling = val.Pulling.firstTimestamp;
            }
            if(val.Pulled)
            {
              obj.Pulled = val.Pulled.firstTimestamp;
            }
            if(val.Created)
            {
              obj.Created = val.Created.firstTimestamp;
            }
            if(val.Started)
            {
              obj.Started = val.Started.firstTimestamp;
            }
            if(val.Killing)
            {
              obj.Killing = val.Killing.firstTimestamp;
            }

            return obj;
          }
        });
        function getRandomColor() {
          var letters = '0123456789ABCDEF';
          var color = '#';
          for (var i = 0; i < 6; i++) {
            color += letters[Math.floor(Math.random() * 16)];
          }
          return color;
        }
        var allData = [];
        var FailedScheduling = [];
        var scheduled = [];
        var SuccessfulMountVolume = [];
        var Pulling = [];
        var Pulled = [];
        var Created = [];
        var Started = [];
        var Killing = [];
        var labels= [];
        for(i=0;i<result.length;i++)
        {
          var row2 = [];
          FailedScheduling.push(result[i].FailedScheduling);
          scheduled.push(result[i].scheduled);
          SuccessfulMountVolume.push(result[i].SuccessfulMountVolume);
          Pulling.push(result[i].Pulling);
          Pulled.push(result[i].Pulled);
          Created.push(result[i].Created);
          Started.push(result[i].Started);
          Killing.push(result[i].Killing);

          row2.push(result[i].name);
          if(result[i].FailedScheduling)
            row2.push(result[i].FailedScheduling);
          else
            row2.push('none');
          if(result[i].scheduled)
            row2.push(result[i].scheduled);
          else
            row2.push('none');
          if(result[i].SuccessfulMountVolume)
            row2.push(result[i].SuccessfulMountVolume);
          else
            row2.push('none');
          if(result[i].Pulling)
            row2.push(result[i].Pulling);
          else
            row2.push('none');
          if(result[i].Pulled)
            row2.push(result[i].Pulled);
          else
            row2.push('none');
          if(result[i].Created)
            row2.push(result[i].Created);
          else
            row2.push('none');
          if(result[i].Started)
            row2.push(result[i].Started);
          else
            row2.push('none');
          if(result[i].Killing)
            row2.push(result[i].Killing);
          else
            row2.push('none');
          allData.push(row2);
          labels.push(result[i].name);
        }
        var testData = [];

        testData.push(FailedScheduling);
        testData.push(scheduled);
        testData.push(SuccessfulMountVolume);
        testData.push(Pulling);
        testData.push(Pulled);
        testData.push(Created);
        testData.push(Started);
        testData.push(Killing);

        new Dygraph(document.getElementById("podTime"),
          testData,
          {
            rollPeriod: 14,
            labels: labels
          });
        var titlesArr = [];

        titlesArr.push({"title": "name"});
        titlesArr.push({"title": "FailedScheduling"});
        titlesArr.push({"title": "scheduled"});
        titlesArr.push({"title": "SuccessfulMountVolume"});
        titlesArr.push({"title": "Pulling"});
        titlesArr.push({"title": "Pulled"});
        titlesArr.push({"title": "Created"});
        titlesArr.push({"title": "Started"});
        titlesArr.push({"title": "Killing"});


        console.log("Success Send Description");
        var dataAll = {
          "columns": titlesArr,
          "data": allData
        };
        //data=JSON.parse(data);
        $("#podTimeLineData").DataTable(dataAll);
        $("#overlaypodTimeLineDataid").hide("slow");
        console.log(result);
      }
    );
  </script>

